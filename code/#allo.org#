#+begin_src R
  x <- 1:100
  a <- seq(-.2, -1, -.2)
  b <- seq(1.2,2, .2)
  dq <- expand.grid(x = x, a = a,b = b)
  dq <- mutate(dq, y = a*(x^2)/100 + b * x)

#+end_src

#+RESULTS:

#+begin_src R :exports results :results graphics :file figs/understand_quad.png
  ggplot(dq, aes(x = x, y = y)) +
      geom_line() +
      facet_grid(a~ b)
#+end_src

