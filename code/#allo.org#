  ggplot(predictions_apprange, aes(x = x, y = predicted_Cdia, group = SpCode)) +
      geom_line() +
      facet_wrap(~Region, scales = "free")

#+end_src

#+RESULTS:
[[file:../figs/predictions_byRegion.png]]

