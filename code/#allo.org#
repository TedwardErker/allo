  dsDBH_min_max = ds %>%
      group_by(SpCode) %>%
      summarize(minDBH = min(DBH, na.rm = T),
                maxDBH = max(DBH, na.rm = T)) %>%
      data.frame()

  newdata <- lapply(1:nrow(dsDBH_min_max), function(i) {
      x <- seq(dsDBH_min_max$minDBH[i], dsDBH_min_max$maxDBH[i],  (dsDBH_min_max$maxDBH[i] - dsDBH_min_max$minDBH[i]) / 9)
